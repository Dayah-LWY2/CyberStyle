<div class="profile-info">
  <label for="username">Username</label>
  <input type="text" id="username" value="<%= user.username %>" disabled>

  <label for="email">Email</label>
  <input type="email" id="email" value="<%= user.email %>" disabled>

  <label for="phone">Phone</label>
  <input type="text" id="phone" value="<%= user.phone %>" disabled>

  <label for="gender">Gender</label>
  <input type="text" id="gender" value="<%= user.gender %>" disabled>

  <label for="dob">Date of Birth</label>
  <input type="date" id="dob" value="<%= user.dob %>" disabled>

  <label for="address">Address</label>
  <textarea id="address" disabled><%= user.address %></textarea>
</div>

<div class="profile-buttons">
  <button id="edit-btn" class="profile-button profile-edit-btn">Edit</button>
  <button id="save-btn" class="profile-button profile-save-btn profile-hidden">Save</button>
</div>

<script src="/js/script.js"></script>